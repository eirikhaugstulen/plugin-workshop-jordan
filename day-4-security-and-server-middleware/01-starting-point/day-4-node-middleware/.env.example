# Server port (optional, defaults to 3005)
PORT=3005

# Personal Access Token for authenticating with DHIS2 API
# Generate this in DHIS2: User Profile → Personal Access Tokens → Generate new token
# Select "Server / script context" token type
PERSONAL_ACCESS_TOKEN=your_personal_access_token_here

# Internal API Key for securing this middleware
# Generate using: node generate-api-token.js
# Add this same key to your DHIS2 route configuration
INTERNAL_API_KEY=your_generated_api_key_here
